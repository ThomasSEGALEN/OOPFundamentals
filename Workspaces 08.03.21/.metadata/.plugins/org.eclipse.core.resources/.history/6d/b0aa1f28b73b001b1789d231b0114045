package fr.campusacademy.oopcourse.interfaces.launcher;

import fr.campusacademy.oopcourse.interfaces.model.Movable;
import fr.campusacademy.oopcourse.interfaces.model.MovableCircle;
import fr.campusacademy.oopcourse.interfaces.model.MovablePoint;
import fr.campusacademy.oopcourse.interfaces.model.MovableRectangle;

public class Launcher {

	public static void main(String[] args) {

		Movable movable1 = new MovablePoint(5, 6, 10, 4);
		System.out.println(movable1);
		movable1.moveLeft();
		System.out.println(movable1);

		System.out.println("");

		Movable movable2 = new MovableCircle(2, 1, 2, 4, 20);
		System.out.println(movable2);
		movable2.moveRight();
		System.out.println(movable2);

		System.out.println("");

		Movable movable3 = new MovableRectangle(3, 3, 1, 1, 5, 5);
		System.out.println(movable3);
		movable3.topLeft();
		System.out.println(movable3);
	}

}