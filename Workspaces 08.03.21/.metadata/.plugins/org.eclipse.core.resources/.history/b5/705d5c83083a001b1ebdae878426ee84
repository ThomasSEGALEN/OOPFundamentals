public class Launcher {

	public static void main(String[] args) {

//		Car.nbOfWheels = 6;
		Car.printAllCarBrand();

		Car myCar1 = new Car();
		myCar1.color = "Yellow";
		myCar1.price = 59000.0;
		myCar1.brand = "BMW";

//		myCar1.stopEngine();
		myCar1.startEngine();

		Car myCar2 = new Car();
		myCar2.color = "Blue";
		myCar2.price = 28000.0;
		myCar2.brand = "Renault";

//		myCar2.startEngine();
		myCar2.stopEngine();

	}

}