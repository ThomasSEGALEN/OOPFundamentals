package fr.campusacademy.oopcourse.inheritance.model;

public class Person {

	protected String name;
	protected int age;
	protected String gender;

	public Person(String name, int age, String gender) {

		this.name = name;
		this.age = age;
		this.gender = gender;
	}

	@Override
	public String toString() {

		return name + ", age:" + age + ", gender:" + gender;
	}

}