public class Launcher {

	public static void main(String[] args) {

		int myInt = 10;
		changeMyInt(myInt);
		System.out.println(myInt);

		Cat cat = new Cat("White");
		changeMyCat(cat);
		System.out.println(cat.color);
	}

	static void changeMyInt(int myInt) {

		myInt = 0;
	}

	static void changeMyCat(Cat cat) {

		cat.color = "Black";
	}
}
